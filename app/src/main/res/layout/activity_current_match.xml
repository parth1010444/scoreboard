        <TextView
            android:id="@+id/NonStrikerName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:layout_marginTop="10dp"
            android:text="TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/StrikerName" />

        <TextView
            android:id="@+id/NonStrikerRuns"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/NonStrikerBallsPlayed"
            app:layout_constraintStart_toEndOf="@+id/NonStrikerName"
            app:layout_constraintTop_toBottomOf="@+id/StrikerRuns" />

        <TextView
            android:id="@+id/NonStrikerBallsPlayed"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/NonStrikerSix"
            app:layout_constraintStart_toEndOf="@+id/NonStrikerName"
            app:layout_constraintTop_toBottomOf="@+id/StrikerBallsPlayed" />

        <TextView
            android:id="@+id/NonStrikerFours"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/NonStrikerSix"
            app:layout_constraintStart_toEndOf="@+id/NonStrikerBallsPlayed"
            app:layout_constraintTop_toBottomOf="@+id/StrikerFour" />

        <TextView
            android:id="@+id/NonStrikerSix"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.466"

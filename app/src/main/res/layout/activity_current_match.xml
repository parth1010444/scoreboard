            app:layout_constraintEnd_toStartOf="@+id/StrikerSix"
            app:layout_constraintStart_toEndOf="@+id/StrikerName"
            app:layout_constraintTop_toBottomOf="@+id/Balls" />

        <TextView
            android:id="@+id/StrikerFour"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/StrikerSix"
            app:layout_constraintStart_toEndOf="@+id/StrikerBallsPlayed"
            app:layout_constraintTop_toBottomOf="@+id/fours" />

        <TextView
            android:id="@+id/StrikerSix"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="12dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintStart_toEndOf="@+id/StrikerName"
            app:layout_constraintTop_toBottomOf="@+id/sixes" />

        <TextView
            android:id="@+id/StrikerSR"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.424"
            app:layout_constraintStart_toEndOf="@+id/StrikerSix"
            app:layout_constraintTop_toBottomOf="@+id/SR" />

        <TextView
            android:id="@+id/StrikerRuns"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="16dp"
            android:text="TextView"
            app:layout_constraintEnd_toStartOf="@+id/StrikerBallsPlayed"
            app:layout_constraintStart_toEndOf="@+id/StrikerName"
            app:layout_constraintTop_toBottomOf="@+id/Runs" />

        <TextView
            android:id="@+id/Balls"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="20dp"
            android:text="B"
            app:layout_constraintEnd_toStartOf="@+id/fours"
            app:layout_constraintHorizontal_bias="0.76"
            app:layout_constraintStart_toEndOf="@+id/batsman"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/fours"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="20dp"
            android:text="4s"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.334"
            app:layout_constraintStart_toEndOf="@+id/batsman"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/sixes"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="24dp"
            android:text="6s"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.099"
            app:layout_constraintStart_toEndOf="@+id/fours"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/SR"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="20dp"
            android:text="SR"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.424"
            app:layout_constraintStart_toEndOf="@+id/sixes"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Runs"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginTop="20dp"
            android:text="R"
            app:layout_constraintEnd_toStartOf="@+id/Balls"
            app:layout_constraintStart_toEndOf="@+id/batsman"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="368dp"
        android:layout_height="41dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <TextView
            android:id="@+id/textView67"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>